{
    "name": "goalgorilla/social_template",
    "description": "Social project template for composer based Open Social projects.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "goalgorilla/open_social": "~10.0.0",
        "php": "^7.2",
        "monolog/monolog": "^1.17",
        "drupal/subgroup": "^1.0",
        "drupal/aggrid": "^1.0@alpha",
        "drupal/commerce": "^2.23",
        "drupal/social_course": "^2.13",
        "drupal/social_geolocation": "^1.2",
        "drupal/social_pwa": "^1.0@alpha",
        "drupal/feeds": "^3.0@alpha",
        "drupal/book_pdf": "^1.0@alpha",
        "drupal/fullcalendar_view": "^5.0",
        "drupal/friggeri_cv": "^1.0@alpha"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/social": [
                "goalgorilla/open_social"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}

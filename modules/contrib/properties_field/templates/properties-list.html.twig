{#
/**
 * @file
 * Default properties list template.
 *
 * Available variables:
 * - items: The list items, each entry contains a "label" and "value" key.
 * - attributes: HTML attributes for the definition list.
 */
#}
{% if items %}
  <dl{{ attributes }}>
    {% for item in items %}
      <dt>{{ item.label }}</dt>
      <dd>{{ item.value }}</dd>
    {% endfor %}
  </dl>
{% endif %}

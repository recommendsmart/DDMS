{#
/**
 * @file entity_collection.html.twig
 * Default theme implementation to present Entity collection data.
 *
 * This template is used when viewing Entity collection pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_entity_collection()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('entity_collection') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>

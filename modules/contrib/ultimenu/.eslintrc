{
  // This file extends .eslintrc at drupal root.
  // Located at /modules/contrib/ultimenu, run:
  // eslint . -o ../../eslint/ultimenu.html -f html
  "extends": "../../../core/.eslintrc.legacy.json",
  "globals": {
    "jQuery": true
  },
  "rules": {
    "consistent-this": [0, "me"],
    "max-params": 0,
    "id-length": [1, {"min": 1}],
    // Warnings.
    "max-nested-callbacks": [1, 4]
  }
}
